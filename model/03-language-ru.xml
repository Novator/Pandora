      <building name.ru="Дом" />
      <appartment name.ru="Квартира" />
    </Address>

    <Contact name.ru="Контакт|Контакты">
      <type name.ru="Тип" />
      <value name.ru="Значение" />
      <owner_list name.ru="Владельцы" />
